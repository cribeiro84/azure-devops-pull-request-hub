{
    "extends": [
      "tslint:recommended",
      "tslint-react",
      "tslint-config-prettier"
    ],
    "rules": {
      "quotemark": {
        "options": [
          "double",
          "jsx-double"
        ]
      },
      "member-access": false,
      "object-literal-sort-keys": false,
      "array-type": false,
      "ordered-imports": false,
      "no-shadowed-variable": false,
      "no-console": false,
      "max-classes-per-file": false,
      "member-ordering": [
        false
      ],
      "jsx-no-lambda": false
    },
    "linterOptions": {
      "exclude": [
        "config/**/*.js",
        "node_modules/**/*.ts"
      ]
    }
  }
